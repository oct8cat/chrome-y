{
    "manifest_version": 2,
    "name": "y",
    "description": "Ты супер!",
    "version": "0.0.0",
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["content.js"]
        }
    ],
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}
